<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.palodurobuilders.contractorapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".pages.CreateAccount"
            android:label="Create Account"/>

        <activity android:name=".pages.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".pages.PropertyUtilities"
            android:windowSoftInputMode="stateVisible|adjustResize"
            android:label="Property Utilities" />
        <activity android:name=".pages.PropertySelection"></activity>
    </application>

</manifest>